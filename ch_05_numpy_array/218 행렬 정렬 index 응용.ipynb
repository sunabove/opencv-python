   "source": [
    "import numpy as np\n",
    "\n",
    "a = np.array( [ [1,5], [2, 7], [2, 6 ], [3, 4] ] )\n",
    "aa = np.argsort( a[:,1] )\n",
    "b = a[ aa ]\n",
    "c = b[ np.argsort( b[:,0], kind='stable' ) ]\n",
    "\n",
    "print( 'a = ', a , sep='\\n' )\n",
    "print( 'aa = ', aa , sep='\\n' )\n",
    "print( 'b = ', b , sep='\\n' )\n",
    "print( 'c = ', c , sep='\\n' )"
   ]
  }
